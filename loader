local function showofflabelbody()
autozapiskbody:SetVisible( false )
textobnovbody:SetVisible( false )
podpiskabody:SetVisible( false )
nameskipbody:SetVisible( false )
end
showofflabelbody()

local function showonlabelbody()
autozapiskbody:SetVisible( true )
textobnovbody:SetVisible( true )
podpiskabody:SetVisible( true )
nameskipbody:SetVisible( true )
end



