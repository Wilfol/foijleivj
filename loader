local function checkloadantisgnoti()
if !antisgpnstatusirmg then

http.Fetch( "https://hl2rpclassic.fun/gmodfun/cod/modularesung/moduleantisgjoise.txt",
	
	-- onSuccess function
	function( body, length, headers, code )
		-- The first argument is the HTML we asked for.
		if code == 404 then
--print("КОД ОШИБКИ: E3481 \nНЕМЕДЛЕННО ОТПИШИТЕ ЧЕЛОВЕКУ У КОТОРОГО ПОКУПАЛИ")
MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ), "НЕ УДАЛОСЬ ЗАПУСТИТЬ МОДУЛЬ ANTISCREENGRAB. ОШИБКА A7412", "\n\n")
--RunConsoleCommand("disconnectfalse")
else
		
		local coderunloski = body
//print(coderunloski)
		
		if coderunloski == "" then

		else
		//print(runcodegmod)
		
		if checkssun(coderunloski, "R41544") then -- проверка ссылки
		RunString(coderunloski) else return
		end
		
MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 75, 255, 135 ), "Start module AntiSG\n\n")
modulaseijs = true
		end
		end
	end,

	-- onFailure function
	function( message )
		-- We failed. =(
		--print("КОД ОШИБКИ: E8814 \nНЕМЕДЛЕННО ОТПИШИТЕ ЧЕЛОВЕКУ У КОТОРОГО ПОКУПАЛИ")
		MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ), "НЕ УДАЛОСЬ ЗАПУСТИТЬ МОДУЛЬ ANTISCREENGRAB. ОШИБКА A1426", "\n\n")
		--RunConsoleCommand("disconnectfalse")
	end)


elseif antisgpnstatusirmg then
if !modulaseijs then
MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 75, 255, 135 ), "Start module AntiSG\n\n")
modulaseijs = true
end
end
end


local function checkspeclistload()
if !speclistcfgloadfinish then

http.Fetch( "https://hl2rpclassic.fun/gmodfun/cod/modularesung/modulespeclistisme9ifgr.txt",
	
	-- onSuccess function
	function( body, length, headers, code )
		-- The first argument is the HTML we asked for.
		if code == 404 then
--print("КОД ОШИБКИ: E3481 \nНЕМЕДЛЕННО ОТПИШИТЕ ЧЕЛОВЕКУ У КОТОРОГО ПОКУПАЛИ")
MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ), "НЕ УДАЛОСЬ ЗАПУСТИТЬ МОДУЛЬ SPECTATE LIST. ОШИБКА S7412", "\n \n")
--RunConsoleCommand("disconnectfalse")
else
		
		local speclistcode = body
//print(speclistcode)
		
		if speclistcode == "" then

		else
		//print(speclistcode)
		--RunString(speclistcode)

		if checkssun(speclistcode, "R41144") then -- проверка ссылки
		RunString(speclistcode) else return
		end

MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 75, 255, 135 ), "Start module SpecList\n\n")
modulaseijsspeclist = true
		end
		end
	end,

	-- onFailure function
	function( message )
		-- We failed. =(
		--print("КОД ОШИБКИ: E8814 \nНЕМЕДЛЕННО ОТПИШИТЕ ЧЕЛОВЕКУ У КОТОРОГО ПОКУПАЛИ")
		MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ), "НЕ УДАЛОСЬ ЗАПУСТИТЬ МОДУЛЬ SPECTATE LIST. ОШИБКА S1426", "\n \n")
		--RunConsoleCommand("disconnectfalse")
	end)


elseif speclistcfgloadfinish then
if !modulaseijsspeclist then
MsgC( Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 75, 255, 135 ), "Start module SpecList\n\n")
modulaseijsspeclist = true
end
end
end


