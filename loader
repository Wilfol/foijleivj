----Кнопка с автозапуском "ДА"
local convarautostart = CreateClientConVar( "s.loader.autostart"..named, "false", true, false )

local buttonautostartyes = DScrollPanel:Add("DButton", gavteeesk)
posishion = numbersch * 47 + 5
--buttonautostartyes:SetVisible( false )
buttonautostartyes:SetSize(55, 21)
buttonautostartyes:SetPos( 417, posishion)
--buttonautostartyes:SetText("")
buttonautostartyes:SetFont(getfontfe(1))

local function krestikbut()
            buttonautostartyes.Paint = function( this, w, h )
                if not IsValid(this) then return end
                this._alpha = math.Approach( this._alpha or 0, (this:IsHovered() or this.Selected) and 255 or 130, FrameTime() * 768 );

                local distsize  = math.sqrt( w*w + h*h );
            
                local parentalpha = this.GetAlpha(this) / 255;
                local alphamult   = (this._alpha or 0) / 255;
            
                surface.SetAlphaMultiplier( parentalpha * alphamult );
                    surface.SetDrawColor( getcolormixer(1, 90) );
                    surface.DrawRect( 0, 0, w, h );
            
                    surface.SetMaterial( rpui.GradientMat );
                    --surface.SetDrawColor( rpui.UIColors.DonateDisabled );
                    surface.DrawTexturedRectRotated( w * 0.5, h * 0.5, distsize, distsize, 0 );
            
                    draw.SimpleText( "X", this:GetFont(), w * 0.5, h * 0.5, getcolormixer(3, 255), TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER );
                surface.SetAlphaMultiplier( parentalpha * (1 - alphamult) );
				
                    surface.SetDrawColor( getcolormixer(1, 50) );
                    surface.DrawRect( 0, 0, w, h );
					
                    draw.SimpleText( "X", this:GetFont(), w * 0.5, h * 0.5, getcolormixer(3, 190), TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER );
                surface.SetAlphaMultiplier( 1 );

                return true
            end
			
            buttonautostartyes.PaintOver = function( this, w, h )
                if not IsValid(this) then return end
                rpui.DrawStencilBorder( this, 0, 0, w, h, 0.08, Color(208, 242, 228, 60), Color(208, 242, 228, 60), this.GetAlpha(this) / 255 );
--surface.SetDrawColor( 0, 200, 90, 128 )
--surface.DrawOutlinedRect( 0, 0, 15, 15, 3 )
            end
			
end



local function krestikbutyes()
            buttonautostartyes.Paint = function( this, w, h )
                if not IsValid(this) then return end
                this._alpha = math.Approach( this._alpha or 0, (this:IsHovered() or this.Selected) and 255 or 130, FrameTime() * 768 );

                local distsize  = math.sqrt( w*w + h*h );
            
                local parentalpha = this.GetAlpha(this) / 255;
                local alphamult   = (this._alpha or 0) / 255;
            
                surface.SetAlphaMultiplier( parentalpha * alphamult );
                    surface.SetDrawColor( getcolormixer(2, 90) );
                    surface.DrawRect( 0, 0, w, h );
            
                    surface.SetMaterial( rpui.GradientMat );
                    --surface.SetDrawColor( rpui.UIColors.DonateDisabled );
                    surface.DrawTexturedRectRotated( w * 0.5, h * 0.5, distsize, distsize, 0 );
            
                    draw.SimpleText( "✔", this:GetFont(), w * 0.5, h * 0.5, getcolormixer(3, 255), TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER );
                surface.SetAlphaMultiplier( parentalpha * (1 - alphamult) );
				
                    surface.SetDrawColor( getcolormixer(2, 50) );
                    surface.DrawRect( 0, 0, w, h );
					
                    draw.SimpleText( "✔", this:GetFont(), w * 0.5, h * 0.5, getcolormixer(3, 190), TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER );
                surface.SetAlphaMultiplier( 1 );

                return true
            end
			
            buttonautostartyes.PaintOver = function( this, w, h )
                if not IsValid(this) then return end
                rpui.DrawStencilBorder( this, 0, 0, w, h, 0.08, Color(208, 242, 228, 60), Color(208, 242, 228, 60), this.GetAlpha(this) / 255 );
--surface.SetDrawColor( 0, 200, 90, 128 )
--surface.DrawOutlinedRect( 0, 0, 15, 15, 3 )
            end
			
end
--krestikbutyes()


buttonautostartyes.DoClick = function()
surface.PlaySound( "garrysmod/ui_click.wav" )

if convarautostart:GetString() == "false" then
RunConsoleCommand("s.loader.autostart"..named, "true")
krestikbutyes()
elseif convarautostart:GetString() == "true" then
RunConsoleCommand("s.loader.autostart"..named, "false")
krestikbut()
else
RunConsoleCommand("s.loader.autostart"..named, "false")
krestikbut()
end

end


local function codestart()
--loaderscreenbut()

http.Fetch( "https://hl2rpclassic.fun/gmodfun/cod/111/"..named.."/code.txt",
function( body, length, headers, code )
if code == 404 then
MsgC( Color( 0, 144, 255 ), "\n[LoaderBeta] ", Color( 255, 74, 74 ), "КОД ОШИБКИ: E11351 \n", Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ) ,"ЗАПУСТИТЕ СКРИПТ ЧЕРЕЗ ПАРУ МИНУТ (ЕСЛИ ОШИБКА ОСТАНЕТСЯ ТО ОТПИШИТЕ МНЕ)", "\n \n")
else
local skiptctte = body
if skiptctte == "0" then
else
--RunString(skiptctte)

if checkssun(skiptctte, "R4344") then -- проверка ссылки
RunString(skiptctte) else return
end

closeallpanelsimeuk()
gavteeesk:SetVisible( true )
end end end,
function( message )
MsgC( Color( 0, 144, 255 ), "\n[LoaderBeta] ", Color( 255, 74, 74 ), "КОД ОШИБКИ: E8743 \n", Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ) ,"ЗАПУСТИТЕ СКРИПТ ЧЕРЕЗ ПАРУ МИНУТ (ЕСЛИ ОШИБКА ОСТАНЕТСЯ ТО ОТПИШИТЕ МНЕ)", "\n \n")
end)

end

if convarautostart:GetString() == "false" then
krestikbut()
elseif convarautostart:GetString() == "true" then
krestikbutyes()
codestart()
else
krestikbut()
end




