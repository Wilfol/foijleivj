function loadloader()
local convarbindmenus1 = CreateClientConVar( "loader_menubindnumber", 72, true, false ) -- бинд меню


testpansss = vgui.Create("DFrame") -- главное окно

--gavteeesk = testpansss:Add( "DPanel" ) -- панель главного меню
gavteeesk1 = testpansss:Add( "DPanel" ) -- панель главного меню
local mattest = vgui.Create("Material", gavteeesk1)
gavteeesk = vgui.Create( "DPanel", gavteeesk1 )
local twxxxx = vgui.Create( "DLabel", gavteeesk ) -- текст в главном меню
local buttonreload = vgui.Create("DButton", testpansss) -- кнопка обновить список
local buttonsettings = vgui.Create("DButton", testpansss) -- кнопка настройки
loaderss = gavteeesk1:Add( "DPanel" ) -- панель загрузки
local textloaderscr = vgui.Create( "DLabel", loaderss ) -- текст в загрузке
local panelsetting = testpansss:Add( "DPanel" ) -- панель настройки
