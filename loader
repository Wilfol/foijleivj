function closeallpanelsimeuk()
gavteeesk:SetVisible( false );
loaderss:SetVisible( false );
panelsetting:SetVisible( false );
panelunikid:SetVisible( false );





if agitkamenu then panelframeopenagit:SetVisible( false ) agitkamenu = false end
if fasthealmenu then panelframeopenheal:SetVisible( false ) fasthealmenu = false end
if diskmenu then diskmenuus:SetVisible( false ) diskmenu = false end
if fpsmenuboostload then fpsmenuboostloadss:SetVisible( false ) fpsmenuboostload = false end
if tfafixload then tfafixmodulepant:SetVisible( false ) tfafixload = false end
end


