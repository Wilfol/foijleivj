local convarspecliststatus = CreateClientConVar( "s.loader.speclist", 1, true, false )
local speclistcustom = ui.Create( "DComboBox", panelsetting )
speclistcustom:SetPos( 220, 70 )
speclistcustom:SetSize( 235, 20 )
speclistcustom:SetColor( getcolormixer(3, 200) )
speclistcustom:SetFont(getfontfe(3))
speclistcustom:SetValue( "Spectate List: Выкл." )
speclistcustom:AddChoice( "Spectate List: Выкл." )
speclistcustom:AddChoice( "Spectate List: Вкл." )


local function speclistcutomon()
speclistcustomstatus = false
speclistcustom:SetValue( "Spectate List: Вкл." )
speclistcustom:SetColor( getcolormixer(4, 200) )
speclistcustom.Paint = function( self, w, h)
draw.RoundedBoxEx( 15, 0, 0, w, h, getcolormixer(2, 180), false, false, false, false )
end
end

local function speclistcutomoff()
speclistcustomstatus = true
speclistcustom:SetValue( "Spectate List: Выкл." )
speclistcustom:SetColor( getcolormixer(3, 200) )
speclistcustom.Paint = function( self, w, h)
draw.RoundedBoxEx( 15, 0, 0, w, h, getcolormixer(1, 105), false, false, false, false )
end
end


speclistcustom.OnSelect = function( self, index, value )
RunConsoleCommand("s.loader.speclist", index)
if index == 1 then
speclistcutomoff()
elseif index == 2 then
speclistcutomon()
end
end

if convarspecliststatus:GetInt() == 2 then
speclistcutomon()
else
speclistcutomoff()
end




