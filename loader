-------------------ТИП ПОДПИСКИ И ВРЕМЯ
local function dateinfogetinpost(namesskiprter ,datatextget, namesskeofrkmeo, statusid)

/*
years1, day1, mouth1, testlog, status1, timersdata = ""

statusid = namesskeofrkmeo

--------------ДЕЛАЕТ ЗАПРОС НА МОЙ САЙТ ЧТО БЫ УЗНАТЬ ДО КАКОГО ПОДПИСКА
local svernik = string.reverse( string.Right( ply:SteamID64(), 10 ) / 4)
http.Fetch( "https://testgmod.ucoz.net/"..svernik.."/"..datatextget..".txt",
function( body, length, headers, code )
if code == 404 then
--MsgC( Color( 0, 144, 255 ), "\n[LoaderBeta] ", Color( 255, 74, 74 ), "КОД ОШИБКИ: E113512 \n", Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ) ,"ЗАПУСТИТЕ СКРИПТ ЧЕРЕЗ ПАРУ МИНУТ (ЕСЛИ ОШИБКА ОСТАНЕТСЯ ТО ОТПИШИТЕ МНЕ)", "\n \n")
else


--RunString(body)

if checkssun(body, "R17693") then -- проверка ссылки
RunString(body) else return
end

--PrintTable(testlog)

for k, v in pairs(testlog) do
if k == "day" then
day1 = v
end
if k == "mouth" then
mouth1 = v
end
if k == "years" then
years1 = v
end
if k == "status" then
statusid = v
end
end



-----------------------ЗАПРАШИВАЕТ ВРЕМЯ ДО КАКОГО ПОДПИСКА
http.Post( "https://hl2rpclassic.fun/gmodfun/info/time/time.php", { god = years1.."", mes = mouth1.."", day = day1.."" },
--http.Fetch( "https://showcase.api.linx.twenty57.net/UnixTime/tounix?date="..years1.."/"..mouth1.."/"..day1.."%2001:00:00",
--http.Post( "http://aowalop.beget.tech/time/time.php", { god = years1, mes = mouth1, day = day1 },
--
function( body, length, headers, code )
if code == 404 then
MsgC( Color( 0, 144, 255 ), "\n[LoaderBeta] ", Color( 255, 74, 74 ), "КОД ОШИБКИ: E113513 \n", Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ) ,"ЗАПУСТИТЕ СКРИПТ ЧЕРЕЗ ПАРУ МИНУТ (ЕСЛИ ОШИБКА ОСТАНЕТСЯ ТО ОТПИШИТЕ МНЕ)", "\n \n")
else
local teeest = string.reverse( string.Right( string.reverse( string.Right( body, 11 )) , 10 ))


-------------------ЗАПРАШИВАЕТ ТЕКУЩЕЕ ВРЕМЯ
http.Fetch( "https://hl2rpclassic.fun/gmodfun/info/time/time1.php",
--http://aowalop.beget.tech/time/time1.php
--https://showcase.api.linx.twenty57.net/UnixTime/tounixtimestamp?datetime=now
function( body, length, headers, code )
if code == 404 then
MsgC( Color( 0, 144, 255 ), "\n[LoaderBeta] ", Color( 255, 74, 74 ), "КОД ОШИБКИ: E1135 \n", Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ) ,"ЗАПУСТИТЕ СКРИПТ ЧЕРЕЗ ПАРУ МИНУТ (ЕСЛИ ОШИБКА ОСТАНЕТСЯ ТО ОТПИШИТЕ МНЕ)", "\n \n")
else
local unixtimeloc = body

local teotko = string.Explode( ".", (teeest - unixtimeloc) / 60 / 60 / 24)
for k, v in pairs(teotko) do
if k == 1 then
local timersdata = v + 1

if statusid == "on" then
if timersdata >= 1 then
--namesskeofrkmeo(timersdata, datatextget)

addbuttiins(namesskiprter, datatextget, timersdata)

end
end


--print("Осталось дней: "..timersdata.." "..datatextget)
end
end

--------ОЧИЩАЕТ ВСЕ ПЕРЕМЕННЫЕ

end end,
function( message )
--MsgC( Color( 0, 144, 255 ), "\n[LoaderBeta] ", Color( 255, 74, 74 ), "КОД ОШИБКИ: E8743 \n", Color( 0, 144, 255 ), "[LoaderBeta] ", Color( 255, 74, 74 ) ,"ЗАПУСТИТЕ СКРИПТ ЧЕРЕЗ ПАРУ МИНУТ (ЕСЛИ ОШИБКА ОСТАНЕТСЯ ТО ОТПИШИТЕ МНЕ)", "\n \n")
end)
