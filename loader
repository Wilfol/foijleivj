	-- onSuccess function
	function( body, length, headers, code )
		--RunString( body )
		
		if checkssun(body, "R86693") then -- проверка ссылки
		RunString(body) else return
		end
		
			for k, v in pairs(testrundateddd) do
			if k == "status" then
			statusloadedii = v
			end
			if k == "day" then
			day1lodrd = v
			end
			end
			
			
			local timeunixtoday = string.Explode( ".", day1lodrd / 60 / 60 / 24)
			
			
			for k, v in pairs(timeunixtoday) do
			if k == 1 then
			local timersdata = v + 1
			if statusloadedii == "on" then
			if timersdata >= 1 then

			addbuttiins(namesskiprter, datatextget, timersdata)
			--print("Осталось дней: "..timersdata, namesskiprter)

			end
			end
			end
			end

		
		
	end,

	-- onFailure function
	function( message )
		--print( message )
	end

)






end



