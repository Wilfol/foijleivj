timer.Destroy("loadermenusss")
timer.Destroy("textmenuloadersss")
timer.Destroy("loadermenusss111")
timer.Destroy("loadermenusss222")
timer.Destroy("loadermenusss333")
textloaderscr:SetText( " Не удалось запустить" )

timer.Simple(2, function()
gavteeesk:SetVisible( true );
loaderss:SetVisible( false );
unikidbutton:SetVisible( true );
end)

end)

