--Кнопка скопировать
copybutton:SetSize(115, 30)
copybutton:SetPos( 190, 260 - minyspaik)
--copybutton:SetText("")
copybutton:SetFont(getfontfe(1))
/*
copybutton.Paint = function( self, w, h)
--raw.RoundedBox( 15, 0, 0, w, h, Color(50, 205, 50, 200))
draw.RoundedBoxEx( 15, 0, 0, w, h, Color(0, 0, 0, 200), false, false, false, false )
draw.SimpleText("Скопировать ID", getfontfe(1), w / 2, h / 2.0, Color( 190, 190, 190 ), TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER)
surface.SetDrawColor( getcolormixer(1, 128) )
surface.DrawOutlinedRect( 0, 0, 115, 30, 3 )
end
*/



local function createpainbttonset(name, color1, color2, colordrav, colordrav2, alpha1, alpha2, w, h, this)
		this:SetText("")
                if not IsValid(this) then return end
                this._alpha = math.Approach( this._alpha or 0, (this:IsHovered() or this.Selected) and alpha1 or alpha2, FrameTime() * 768 );

                local distsize  = math.sqrt( w*w + h*h );
            
                local parentalpha = this.GetAlpha(this) / 255;
                local alphamult   = (this._alpha or 0) / 255;
            

                surface.SetAlphaMultiplier( parentalpha * alphamult );
                    surface.SetDrawColor( colordrav );
                    surface.DrawRect( 0, 0, w, h );

            
                    surface.SetMaterial( rpui.GradientMat );

                    --surface.SetDrawColor( rpui.UIColors.DonateDisabled );
                    surface.DrawTexturedRectRotated( w * 0.5, h * 0.5, distsize, distsize, 0 );
            

                    draw.SimpleText( name, this:GetFont(), w * 0.5, h * 0.5, color2, TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER );
                surface.SetAlphaMultiplier( parentalpha * (1 - alphamult) );

                    surface.SetDrawColor( colordrav2 );
                    surface.DrawRect( 0, 0, w, h );

                    draw.SimpleText( name, this:GetFont(), w * 0.5, h * 0.5, color1, TEXT_ALIGN_CENTER, TEXT_ALIGN_CENTER );
                surface.SetAlphaMultiplier( 1 );


                return true
end



            copybutton.Paint = function( this, w, h )
				createpainbttonset(
					"Скопировать ID",
					getcolormixer(3, 190),
					getcolormixer(3, 255),
					getcolormixer(2, 110),
					getcolormixer(4, 200),
					130,
					0,
					w,
					h,
					this
				)
            end
			
            copybutton.PaintOver = function( this, w, h )
                if not IsValid(this) then return end
                --rpui.DrawStencilBorder( this, 0, 0, w, h, 0.1, Color(255, 0, 0, 90), Color(255, 0, 0, 90), this.GetAlpha(this) / 255 );
surface.SetDrawColor( getcolormixer(1, 128) )
surface.DrawOutlinedRect( 0, 0, 115, 30, 3 )
            end




copybutton.DoClick = function()
surface.PlaySound( "garrysmod/ui_click.wav" )
SetClipboardText( svernik )
rp.Notify(NOTIFY_BLUE, "Уникальный ID был скопирован в буфер обмена")

if antisgpnstatusirmg then
offnotisg() -- антисг уведомления
end

end 
copybutton:SetVisible( true )





