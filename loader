buttonresetcolorss:SetSize(115, 40)
buttonresetcolorss:SetPos( 280, 200)
buttonresetcolorss:SetFont(getfontfe(1))
            buttonresetcolorss.Paint = function( this, w, h )
				createpainbttonset(
					"Сбросить цвета",
					getcolormixer(3, 190),
