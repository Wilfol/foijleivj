--полосы между функциями
testpansgbuhu.Paint = function(self, w, h)
colorsss1 = 8
colorsss2 = 442
pos2 = 37

surface.SetDrawColor( 180, 25, 30, 128 ) -- красный цвет

for i = 1,numbersch do
--for i = 1,7 do
surface.DrawLine( colorsss1, pos2 + 0, colorsss2, pos2 + 0 ) -- дополнительная полоса под кнопками
surface.DrawLine( colorsss1, pos2 + 1, colorsss2, pos2 + 1 ) -- дополнительная полоса под кнопками
surface.DrawLine( colorsss1, pos2 + 2, colorsss2, pos2 + 2 ) -- дополнительная полоса под кнопками
pos2 = pos2 + 47
end
end
--длинна панели с полосами
testpansgbuhu:SetHeight( 50 * numbersch + 50 )



