end)




end

twxxxx:SetVisible( true )
function addnumbers()

if numbersch == 0 then
twxxxx:SetText( "           У вас нет активных подписок!!!!" )
twxxxx:SetVisible( true )
showofflabelbody()
elseif numbersch == 1 then
twxxxx:SetText( "           У вас 1 активная подписка :)" )
showonlabelbody()
twxxxx:SetVisible( false )
elseif numbersch >= 2 && numbersch <= 4 then
twxxxx:SetText( "           У вас "..numbersch.." активные подписки :) :)" )
showonlabelbody()
twxxxx:SetVisible( false )
elseif numbersch >= 5 then
twxxxx:SetText( "           У вас "..numbersch.." активных подписок :) :) :)" )
showonlabelbody()
twxxxx:SetVisible( false )
end

end

















